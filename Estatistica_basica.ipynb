    {
      "cell_type": "code",
      "source": [
        "plt.figure(figsize=(8, 6))\n",
        "plt.pie([dados[dados < 50].count(), dados[dados >= 50].count()], labels=['Menor que 50','Maior ou igual a 50'], autopct='%1.1f%%', colors=['blue', 'orange'])\n",
        "plt.title('Gráfico de Pizza')\n",
        "plt.show()"
